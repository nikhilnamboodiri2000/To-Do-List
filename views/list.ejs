<%- include("header"); -%>
    <div class="box" id="heading">
        <h1>
            <%= kind %> List
        </h1>
    </div>

    <div class="box">

        <% for(var i=0;i<listitem.length;i++){ %>
            <div class="item">
                <input type="checkbox">
                <p>
                    <%= listitem[i] %>
                </p>
            </div>

            <% } %>



                <form class="item" method="POST" action="/">
                    <input type="text" name="newitem" placeholder="New Item" autocomplete="off">
                    <button type="submit" name="b" value="<%= kind %>">+</button>
                </form>
    </div>

    <%- include("footer"); -%>